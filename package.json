{
  "name": "mcp-websearch-server",
  "version": "1.3.1",
  "description": "MCP Server for web search (SearchAPI.io) and advanced web scraping",
  "main": "index.js",
  "type": "module",
  "bin": {
    "mcp-websearch": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "start:http": "MCP_TRANSPORT=http node index.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop mcp-websearch",
    "pm2:restart": "pm2 restart mcp-websearch",
    "pm2:logs": "pm2 logs mcp-websearch"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "web-search",
    "web-scraper",
    "searchapi",
    "ai-search",
    "claude",
    "kiro"
  ],
  "author": "galangw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/galangw/mcp-websearch.git"
  },
  "homepage": "https://github.com/galangw/mcp-websearch#readme",
  "bugs": {
    "url": "https://github.com/galangw/mcp-websearch/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
